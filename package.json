{
  "name": "rest-express",
  "version": "2.1.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:render": "npm ci --include=dev && npm run build",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "type-check": "tsc --noEmit",
    "db:push": "drizzle-kit push",
    "test": "vitest",
    "test:unit": "vitest run server/__tests__/unit",
    "test:integration": "vitest run server/__tests__/integration",
    "test:e2e": "vitest run server/__tests__/e2e",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "security:scan": "tsx server/scripts/detectCredentials.ts",
    "security:scan-strict": "tsx server/scripts/detectCredentials.ts --fail-on-high",
    "migrate:security": "node --import tsx/esm --env-file=.env server/scripts/runSecurityMigration.ts",
    "reset:demo": "node --import tsx/esm --env-file=.env server/scripts/reset-demo-user.ts",
    "validate:security": "tsx server/scripts/validateSecuritySchema.ts",
    "security:maintenance": "tsx -e \"import { runSecurityMaintenance } from './server/scripts/runSecurityMigration.js'; await runSecurityMaintenance();\"",
    "test:migration": "tsx server/scripts/testMigrationSyntax.ts",
    "security:checklist": "tsx server/scripts/securityChecklist.ts",
    "deployment:validate": "tsx server/scripts/deploymentValidation.ts",
    "deployment:build": "npm run build && npm run security:checklist",
    "deployment:production": "cross-env NODE_ENV=production npm run deployment:validate && npm start",
    "cleanup": "node scripts/cleanup.cjs",
    "project:validate": "npm run cleanup && npm run security:checklist",
    "build:analyze": "vite build --mode analyze",
    "db:migrate:performance": "tsx server/scripts/run-performance-migration.ts",
    "db:migrate:stripe": "tsx server/scripts/run-stripe-migration.ts",
    "test:lighthouse": "tsx server/__tests__/performance/lighthouse-audit.ts",
    "test:load": "tsx server/__tests__/performance/load-testing.ts",
    "test:cache": "vitest run server/__tests__/performance/cache-effectiveness.test.ts",
    "test:performance": "npm run test:cache && npm run test:load && npm run test:lighthouse"
  },
